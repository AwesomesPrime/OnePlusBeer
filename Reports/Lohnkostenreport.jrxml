	<subDataset name="Arbeitszeiten" uuid="f8166dc8-2bb5-4ad1-8705-cecb8bc98553">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Localhost"/>
		<parameter name="prmEmployeeId" class="java.lang.Integer"/>
		<parameter name="prmStartDate" class="java.sql.Date"/>
		<parameter name="prmEndDate" class="java.sql.Date"/>
		<queryString>
			<![CDATA[SELECT
	DATE_FORMAT(CAST(oneplusbier.resourceplanning.startWorkingTime AS DATE), '%d.%m') AS workDate
	,oneplusbier.resourceplanning.startWorkingTime
	,oneplusbier.resourceplanning.endWorkingTime
	,TIMEDIFF(TIMEDIFF(oneplusbier.resourceplanning.endWorkingTime, oneplusbier.resourceplanning.startWorkingTime), SEC_TO_TIME(oneplusbier.resourceplanning.pauseTime * 60)) AS Arbeitszeit
FROM oneplusbier.employeeplan AS resourceplanning
INNER JOIN oneplusbier.employee
ON oneplusbier.resourceplanning.employee = oneplusbier.employee.id
WHERE oneplusbier.employee.id =  $P{prmEmployeeId}
AND (DATE(oneplusbier.resourceplanning.startWorkingTime) >=  $P{prmStartDate}  AND DATE(oneplusbier.resourceplanning.startWorkingTime) <=   $P{prmEndDate}  )]]>
		</queryString>
		<field name="workDate" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="workDate"/>
		</field>
		<field name="startWorkingTime" class="java.sql.Timestamp">
			<property name="com.jaspersoft.studio.field.label" value="startWorkingTime"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="employeeplan"/>
		</field>
		<field name="endWorkingTime" class="java.sql.Timestamp">
			<property name="com.jaspersoft.studio.field.label" value="endWorkingTime"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="employeeplan"/>
		</field>
		<field name="Arbeitszeit" class="java.sql.Time">
			<property name="com.jaspersoft.studio.field.label" value="Arbeitszeit"/>
		</field>
	</subDataset>
	<subDataset name="Fahrtkosten" uuid="91659383-5c65-466f-a122-4105a2d5faeb">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Localhost"/>
		<parameter name="prmEmployeeId" class="java.lang.Integer"/>
		<parameter name="prmStartDate" class="java.sql.Date"/>
		<parameter name="prmEndDate" class="java.sql.Date"/>
		<queryString language="SQL">
			<![CDATA[SELECT
   oneplusbier.resourceplanning.travelDistance,
   oneplusbier.resourceplanning.travelStart,
   oneplusbier.event.city AS Veranstaltungsort,
   oneplusbier.resourceplanning.comment 
FROM
   oneplusbier.employeeplan AS resourceplanning
       LEFT JOIN oneplusbier.standplan
       ON oneplusbier.standplan.id = oneplusbier.resourceplanning.standplan
   LEFT JOIN
      oneplusbier.event 
      ON oneplusbier.standplan.event = oneplusbier.event.id
    	
WHERE
   oneplusbier.resourceplanning.employee = $P{prmEmployeeId} 
   AND 
   (
      DATE(oneplusbier.resourceplanning.startWorkingTime) >= $P{prmStartDate} 
      AND DATE(oneplusbier.resourceplanning.startWorkingTime) <= $P{prmEndDate} 
   )]]>
		</queryString>
		<field name="travelDistance" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.label" value="travelDistance"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="employeeplan"/>
		</field>
		<field name="travelStart" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="travelStart"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="employeeplan"/>
		</field>
		<field name="Veranstaltungsort" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="Veranstaltungsort"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="event"/>
		</field>
		<field name="comment" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="comment"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="employeeplan"/>
		</field>
	</subDataset>
	<parameter name="prmEmployeeId" class="java.lang.Integer"/>
	<parameter name="prmStartDate" class="java.sql.Date"/>
	<parameter name="prmEndDate" class="java.sql.Date"/>
	<queryString language="SQL">
		<![CDATA[SELECT
	oneplusbier.resourceplanning.employee
	,oneplusbier.employee.street
	,CONCAT(oneplusbier.employee.plz, " ", oneplusbier.employee.city) AS Ort
	,CONCAT(oneplusbier.employee.firstName, ", ", oneplusbier.employee.lastName) AS Name
	,oneplusbier.employee.bruttoPerHour
	,CONCAT(
	CAST(SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime, startWorkingTime + INTERVAL pauseTime MINUTE))) / 3600 AS Int)
	,":"
	,CAST((SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime, startWorkingTime + INTERVAL pauseTime MINUTE))) % 3600) / 60 AS Int)
	) AS Gesamtzeit
	,LEFT(SEC_TO_TIME(SUM(oneplusbier.resourceplanning.pauseTime*60)),5) AS Pause
	,CAST(SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime,  startWorkingTime + INTERVAL pauseTime MINUTE))) / 3600 AS INT) * oneplusbier.employee.bruttoPerHour
	+ CAST((SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime,  startWorkingTime + INTERVAL pauseTime MINUTE))) % 3600) / 60 AS INT) / 60 * oneplusbier.employee.bruttoPerHour AS Lohn
	,COUNT(oneplusbier.resourceplanning.id) AS RowNumber
	,SUM(travelDistance) AS kilometerGesamt
	,SUM(travelDistance) * 0.3 AS fKosten
    ,SUM(bonus) AS Zulage
    ,(SUM(bonus)
    + IFNULL(CAST(SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime,  startWorkingTime + INTERVAL pauseTime MINUTE))) / 3600 AS INT),0) * oneplusbier.employee.bruttoPerHour
    + IFNULL(CAST((SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime,  startWorkingTime + INTERVAL pauseTime MINUTE))) % 3600) / 60 AS INT) / 60,0) * oneplusbier.employee.bruttoPerHour
    ) AS BruttoLohn
    ,(SUM(bonus)
    + IFNULL(CAST(SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime,  startWorkingTime + INTERVAL pauseTime MINUTE))) / 3600 AS INT),0) * oneplusbier.employee.bruttoPerHour
    + IFNULL(CAST((SUM(TIME_TO_SEC(TIMEDIFF(endWorkingTime,  startWorkingTime + INTERVAL pauseTime MINUTE))) % 3600) / 60 AS INT) / 60,0) * oneplusbier.employee.bruttoPerHour
    + IFNULL(SUM(travelDistance),0) * 0.3
    ) AS GesamtLohn
FROM oneplusbier.employee
LEFT JOIN oneplusbier.employeeplan AS resourceplanning
ON oneplusbier.resourceplanning.employee = oneplusbier.employee.id
WHERE oneplusbier.employee.id = $P{prmEmployeeId} 
 AND (DATE(oneplusbier.resourceplanning.startWorkingTime) >= $P{prmStartDate} AND DATE(oneplusbier.resourceplanning.startWorkingTime) <=$P{prmEndDate})]]>
	</queryString>
